<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <UsingTask TaskName="GenerateScript" AssemblyFile="$(MSBuildThisFileDirectory)H5.Build.dll" />

  <PropertyGroup>
    <NoStdLib>True</NoStdLib>
  </PropertyGroup>

  <ItemGroup>
    <Reference Include="H5.dll">
      <HintPath>$(MSBuildThisFileDirectory)H5.dll</HintPath>
    </Reference>
  </ItemGroup>

  <Target Name="AfterBuild">
    <GenerateScript
      OutputPath="$(OutputPath)"
	    Configuration="$(Configuration)"
      Assembly="@(IntermediateAssembly)"
	    AssembliesPath="$(MSBuildThisFileDirectory)"
      ProjectPath="$(MSBuildProjectFullPath)">
    </GenerateScript>
  </Target>
</Project>
